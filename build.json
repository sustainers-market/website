{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/npm",
      "entrypoint": "bash",
      "args": ["-c", "npm install"]
    },
    {
      "name": "gcr.io/cloud-builders/gcloud",
      "entrypoint": "bash",
      "args": ["-c", "ng build --prod"]
    },
    {
      "name": "gcr.io/cloud-builders/gcloud",
      "entrypoint": "bash",
      "args": ["-c", "gcloud app deploy gcp --verbosity=debug"]
    }
  ]
}
